#version 130
in vec3 inPosition;
in vec4 inColour;
in vec2 inTex;
in vec3 inNorm;
uniform mat4 transformationMatrix;
uniform mat4 projectionMatrix;
uniform mat4 objectMatrix;
out vec2 tex;
void main()
{
  gl_Position = projectionMatrix * transformationMatrix * objectMatrix* vec4(inPosition, 1.0);
  tex = inTex;
}
